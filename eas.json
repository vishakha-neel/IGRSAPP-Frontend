{
  "cli": {
    "version": ">= 16.3.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal"
    },
    "production": {
          "android": {
        "buildType": "apk"
      },
      "autoIncrement": true,
      "distribution": "store",
      "env": {
        "BASE_URL": "https://cgigrsdocument.com/igrsapp"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
   